			When [hour] = 0 Then  '12am to 1am'
			When [hour] = 1 Then  '1am to 2am'
			When [hour] = 3 Then  '3am to 4am'
			When [hour] = 4 Then  '4am to 5am'
			When [hour] = 5 Then  '5am to 6am'
			When [hour] = 7 Then  '7am to 8am'
